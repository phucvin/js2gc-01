(module
 (type $0 (func (param i32) (result i32)))
 (export "factorial" (func $factorial))
 (func $factorial (param $0 i32) (result i32)
  (local $1 i32)
  (local $2 i32)
  (local.set $1
   (i32.const 1)
  )
  (local.set $2
   (i32.const 1)
  )
  (block $loop_out
   (loop $loop
    (br_if $loop_out
     (i32.gt_u
      (local.get $2)
      (local.get $0)
     )
    )
    (local.set $1
     (i32.mul
      (local.get $1)
      (local.get $2)
     )
    )
    (local.set $2
     (i32.add
      (local.get $2)
      (i32.const 1)
     )
    )
    (br $loop)
   )
  )
  (local.get $1)
 )
)
